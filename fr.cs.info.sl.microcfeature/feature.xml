<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="fr.cs.info.sl.microcfeature"
      label="MicroCfeature"
      version="1.0.0.qualifier"
      provider-name="CentraleSupélec, Frédéric Boulanger">

   <description url="http://www.example.com/description">
      Various plug-ins to edit Micro-C files for Why3, and export definitions and lemmas to Isabelle
   </description>

   <copyright url="http://www.example.com/copyright">
      © CentraleSupélec, by Frédéric Boulanger, 2020
   </copyright>

   <license url="http://www.example.com/license">
      This software is distributed under the Eclipse Public License v2.0
https://www.eclipse.org/legal/epl-2.0/
   </license>

   <url>
      <update label="Why3 MicroC feature update site" url="https://wdi.centralesupelec.fr/boulanger/misc/microc-update-site/"/>
      <discovery label="Web page of the author" url="http://wdi.centralesupelec.fr/boulanger"/>
   </url>

   <requires>
      <import plugin="org.eclipse.xtext"/>
      <import plugin="org.eclipse.xtext.xbase"/>
      <import plugin="org.eclipse.equinox.common" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore"/>
